{
  "name": "tech-matchup-app",
  "version": "1.0.0",
  "description": "",
  "main": "server/server.js",
  "scripts": {
    "start": "nodemon server/server.js --ignore client",
    "develop": "concurrently \"node server/server.js\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "node server/seeds/seed.js",
    "build": "cd client && npm run build",
    "start-production": "NODE_ENV=production && concurrently \"node server/server.js\" \"cd client && npm start\"",
    "heroku-postbuild": "npm run install && npm run build"
  },
  "engines": {
    "node": "14.x"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bootstrap": "^5.3.0",
    "gsap": "^3.12.1"
  }
}
